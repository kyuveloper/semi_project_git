<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.semiproject.pettales.map.dao.MapDAO">

    <resultMap id="locationResult" type="com.semiproject.pettales.map.dto.MapDTO">
        <id property="companyCode" column="COMPANY_CODE"/>
        <result property="companyName" column="COMPANY_NAME"/>
        <result property="companyLat" column="COMPANY_LAT"/>
        <result property="companyHar" column="COMAPNY_HAR"/>
    </resultMap>

    <select id="comLocation" resultMap="locationResult">
        SELECT
        COMPANY_NAME,
        COMPANY_LAT,
        COMAPNY_HAR
        FROM COMPANY_TABLE
    </select>


</mapper>